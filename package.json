{
    "name": "tinybird-syntax-support",
    "displayName": "Tinybird support",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tinybird",
            "aliases": ["Tinybird project files", "tinybird"],
            "extensions": [".datasource", ".pipe", ".endpoint"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [
        {
            "language": "tinybird",
            "scopeName": "source.datasource",
            "path": "./syntaxes/tinybird.tmLanguage.json"
        }
        ]
    }
}